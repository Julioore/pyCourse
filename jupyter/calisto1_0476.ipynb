     "text": [
      "5 ×  0 =  0\n",
      "5 ×  1 =  5\n",
      "5 ×  2 = 10\n",
      "5 ×  3 = 15\n",
      "5 ×  4 = 20\n",
      "5 ×  5 = 25\n",
      "5 ×  6 = 30\n",
      "5 ×  7 = 35\n",
      "5 ×  8 = 40\n",
      "5 ×  9 = 45\n",
      "5 × 10 = 50\n"
     ]
    }
   ],
   "source": [
    "def multiplica(n):\n",
    "    for i in range(11):\n",
    "        print(f\"{n} × {i:2} = {n*i:2}\")\n",
    "    \n",
    "multiplica(5)"
